<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE policyconfig PUBLIC
        "-//freedesktop//DTD PolicyKit Policy Configuration 1.0//EN"
        "http://www.freedesktop.org/standards/PolicyKit/1.0/policyconfig.dtd">
<policyconfig>
    <vendor/>
    <vendor_url/>

    <action id="org.deepin.dde.daemon.enable-readonly-protection">
        <description>Enable system read-only protection</description>
        <message>Authentication required to enable read-only protection.</message>
        <defaults>
            <allow_any>no</allow_any>
            <allow_inactive>no</allow_inactive>
            <allow_active>auth_admin_keep</allow_active>
        </defaults>
    </action>
    <action id="org.deepin.dde.daemon.disable-readonly-protection">
        <description>Disable system read-only protection</description>
        <message>Authentication required to disable read-only protection.</message>
        <defaults>
            <allow_any>no</allow_any>
            <allow_inactive>no</allow_inactive>
            <allow_active>auth_admin_keep</allow_active>
        </defaults>
    </action>

</policyconfig>
